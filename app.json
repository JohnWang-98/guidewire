      "bundleIdentifier": "org.mountsinai.guidewire-aid",
      "buildNumber": "29",
      "requireFullScreen": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "android": {
      "package": "com.icando320.GuidewireAID",
      "versionCode": 2,
      "icon": "./assets/ic_launcher.png",
      "adaptiveIcon": {
        "foregroundImage": "./assets/ic_launcher_foreground.png",
        "backgroundColor": "#FFFFFF"
      }
    }
  }
}
